<!DOCTYPE html>
<html>
  <head>
    <title>Lion</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <p><%= link_to 'Scenarios', scenarios_path %> <%= link_to 'Proofs', proofs_path %> <%= link_to 'Verbs', verbs_path %>  <%= link_to 'Nouns', nouns_path %> </p>
    <%= yield %>
  </body>
</html>
